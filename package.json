    "jest-cli": "^20.0.4",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.6",
    "url-loader": "^0.5.9"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "__snapshots__"
    ],
    "rootDir": "src",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
